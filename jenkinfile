pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/prafulkharat23/TravelMemory'
            }
        }
        stage('Install') {
            steps {
                sh 'cd backend'
                sh 'npm install'
            }
        }
        stage('Build') {
            steps {
                sh 'cd backend'
                sh 'npm run build'
            }
        }
    }
}